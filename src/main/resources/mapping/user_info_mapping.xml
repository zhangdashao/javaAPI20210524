<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zw.javaapi.Mapper.User_InfoMapper">



    <select id="getUser_info" parameterType="String" resultType="com.zw.javaapi.Entity.User_Info">
        select * from user_info where 1=1 and user_id=#{user_id}
    </select>


</mapper>
